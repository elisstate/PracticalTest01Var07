<tools:TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PracticalTest01Var07MainActivity">
    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        >

        <EditText
            android:id="@+id/first_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:enabled="false"
            android:ems="7"
            android:gravity="center"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:saveEnabled="false" />
        <EditText
            android:id="@+id/second_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:enabled="false"
            android:ems="7"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:saveEnabled="false"
            android:layout_alignParentLeft="true" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >
        <EditText
            android:id="@+id/third_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:enabled="false"
            android:ems="7"
            android:gravity="center"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:saveEnabled="false" />
        <EditText
            android:id="@+id/fourth_edit_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:enabled="false"
            android:ems="7"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:saveEnabled="false"
            android:layout_alignParentLeft="true" />
    </TableRow>
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="1" >


        <Button
            android:id="@+id/set_button"
            android:layout_width="wrap_content"
            android:text="@string/set"/>
    </TableRow>
</tools:TableLayout>